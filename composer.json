{
    "name": "fakeheal/cors-anywhere",
    "description": "cors-anywhere is a php reverse proxy which adds CORS headers to the proxy request.",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "symfony/http-foundation": "7.0.x-dev"
    },
    "require-dev": {
        "rector/rector": "^0.17.1",
        "phpstan/phpstan": "1.11.x-dev",
        "pestphp/pest": "2.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fakeheal\\CorsAnywhere\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivanka Todorova",
            "email": "fakeheal@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "version": "dev-0.0.1",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
